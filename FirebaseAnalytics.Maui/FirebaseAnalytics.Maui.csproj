<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <UseMaui>true</UseMaui>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    <ItemGroup Condition="'$(TargetFramework)' == 'net9.0-android'">
        <PackageReference Include="Xamarin.GooglePlayServices.Base" Version="118.2.1" />
        <PackageReference Include="Xamarin.Firebase.Analytics" Version="123.1.0" />
    </ItemGroup>

    <ItemGroup Condition="'$(TargetFramework)' == 'net9.0-ios'">
        <PackageReference Include="Xamarin.Firebase.iOS.Analytics" Version="10.15.0.1" />
    </ItemGroup>
</Project>
